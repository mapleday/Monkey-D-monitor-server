<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sohu.sns.monitor.common.dao.appInfo.AppInfoDao">
    <select id="getAppInfo" parameterType="String" resultType="com.sohu.sns.monitor.common.module.AppInfo">
        SELECT * FROM sns_web_appInfo
        WHERE appId=#{appId}
    </select>
</mapper>